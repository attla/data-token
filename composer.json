{
    "name": "attla/data-token",
    "description": "Turn everything into a token: randomized or predictable.",
    "keywords": [
        "attla",
        "laravel",
        "data-to-token",
        "tokenizer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "attla/pincryp": "dev-main",
        "hisorange/browser-detect": "^4.5.4",
        "nesbot/carbon": "^2.64",
        "hashids/hashids": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Attla\\DataToken\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "DataToken": "Attla\\DataToken\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-global-assertions": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
